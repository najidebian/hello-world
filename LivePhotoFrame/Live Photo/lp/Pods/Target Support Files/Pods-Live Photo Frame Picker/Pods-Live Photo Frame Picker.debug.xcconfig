FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/MKDropdownMenu" "$PODS_CONFIGURATION_BUILD_DIR/NSGIF" "$PODS_CONFIGURATION_BUILD_DIR/UIGifImage"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/MKDropdownMenu/MKDropdownMenu.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/NSGIF/NSGIF.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/UIGifImage/UIGifImage.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "MKDropdownMenu" -framework "NSGIF" -framework "UIGifImage"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
